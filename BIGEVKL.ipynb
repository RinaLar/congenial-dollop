   "source": [
    "A,B=map(int,input().split())\n",
    "\n",
    "def gcd(A,B):\n",
    "    if A!=0 and B!=0:\n",
    "        div,x,y=gcd(B%A,A)\n",
    "    else:\n",
    "        return (B,0,1)\n",
    "    return (div,y-(B//A)*x,x)\n",
    "\n",
    "a = gcd(A,B)\n",
    "print(f'НОД {a[0]}, x = {a[1]}, y = {a[2]}')\n"
   ]
